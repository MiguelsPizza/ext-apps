{
  "name": "@modelcontextprotocol/ext-apps-http-adapter",
  "version": "1.0.1",
  "license": "MIT",
  "description": "HTTP adapter for MCP Apps.",
  "type": "module",
  "main": "./dist/init.js",
  "exports": {
